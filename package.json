{
  "name": "remix-spa-example",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "main": "main.cjs",
  "author": "observerkei",
  "description": "Favorite collection",
  "license": "MIT",
  "version": "0.1.0",
  "scripts": {
    "build": "remix vite:build",
    "dev": "remix vite:dev --host=0.0.0.0 --port=5173",
    "edev": "concurrently -n \"REMIX,ELECTRON\" -c \"yellow,blue\" --kill-others \"npm run dev\" \"electron .\"",
    "emake": "npm run build && electron-forge make",
    "epack": "electron-forge package",
    "ebuild": "npm run build && npm run epack",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "preview": "vite preview",
    "typecheck": "tsc",
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make"
  },
  "dependencies": {
    "@remix-run/node": "^2.15.1",
    "@remix-run/react": "^2.15.1",
    "http-server": "^14.1.1",
    "isbot": "^4"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.6.0",
    "@electron-forge/maker-deb": "^7.6.0",
    "@electron-forge/maker-rpm": "^7.6.0",
    "@electron-forge/maker-squirrel": "^7.6.0",
    "@electron-forge/maker-zip": "^7.6.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.6.0",
    "@electron-forge/plugin-fuses": "^7.6.0",
    "@electron-toolkit/utils": "^3.0.0",
    "@electron/fuses": "^1.8.0",
    "@emotion/cache": "latest",
    "@emotion/react": "latest",
    "@emotion/server": "latest",
    "@emotion/styled": "latest",
    "@mui/icons-material": "^6.2.0",
    "@mui/material": "latest",
    "@remix-run/dev": "^2.15.1",
    "@types/node": "^22.10.2",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "autoprefixer": "^10.4.19",
    "concurrently": "^9.1.0",
    "electron": "33.2.1",
    "electron-squirrel-startup": "^1.0.1",
    "eslint": "^8.38.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "postcss": "^8.4.38",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-iframe": "^1.8.5",
    "react-responsive": "^10.0.0",
    "tailwindcss": "^3.4.4",
    "tiny-invariant": "^1.3.3",
    "typescript": "^5.1.6",
    "vite": "^5.1.0",
    "vite-tsconfig-paths": "^4.2.1"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
